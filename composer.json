  "require": {
    "php": "^7.4",
    "doctrine/orm": "*",
    "doctrine/doctrine-migrations-bundle": "dev-master",
    "pecee/simple-router": "4.2.0.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3",
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\AppTest\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": "phpcs -p --standard=PSR2 src tests",
    "phpcsf": "phpcbf -p --standard=PSR2 src tests",
    "phpunit": "phpunit --configuration phpunit.xml"
  }
